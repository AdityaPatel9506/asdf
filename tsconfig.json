{
  "extends": "@react-native/typescript-config/tsconfig.json",
  
    "compilerOptions": {
      "moduleResolution": "node",
      "target": "esnext",
      "module": "esnext",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules"]
  
  
}
